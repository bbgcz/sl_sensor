<?xml version="1.0"?>
<package format="2">
  <name>sl_sensor_scanning_gui</name>
  <version>0.0.0</version>
  <description>Buttons that send basic commands to image synchroniser to start/stop scanning process</description>
  <maintainer email="tlam@student.ethz.ch">Lam Teng Foong</maintainer>
  <license>BSD3</license>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>rospy</depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_py</depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>

</package>
